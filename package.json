{
	"name": "vinci",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"dev:webpack": "next dev --no-turbopack",
		"dev:convex": "bun run scripts/convex-log-router.ts",
		"dev:convex:raw": "convex dev",
		"dev:convex:local": "convex dev --local",
		"dev:all": "concurrently --kill-others-on-fail --names next,convex --prefix-colors blue,magenta \"bun run dev\" \"bun run dev:convex\"",
		"dev:all:ci": "concurrently --kill-others-on-fail --names next,convex --prefix-colors blue,magenta \"bun run dev:webpack\" \"bun run dev:convex:local\"",
		"build": "next build",
		"start": "next start",
		"lint": "bunx biome check .",
		"lint:fix": "bunx biome check --write .",
		"format": "bunx biome format --write .",
		"typecheck": "tsc --noEmit",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage",
		"test:convex": "vitest run",
		"test:convex:watch": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:all": "bun run test && bun run test:convex && bun run test:e2e",
		"playwright:install": "playwright install",
		"logs:clear": "rm -rf .logs/*.jsonl",
		"logs:tail:next": "tail -f .logs/next-*.jsonl | bunx pino-pretty",
		"logs:tail:convex": "tail -f .logs/convex-*.jsonl",
		"kill:all": "lsof -ti :3000,:3210,:6790 | xargs kill -9 2>/dev/null || true"
	},
	"dependencies": {
		"@base-ui/react": "^1.0.0",
		"@convex-dev/better-auth": "^0.10.9",
		"@hugeicons/core-free-icons": "^3.1.1",
		"@hugeicons/react": "^1.1.4",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"better-auth": "1.4.9",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"convex": "^1.31.2",
		"lucide-react": "^0.562.0",
		"next": "16.1.1",
		"pino": "^10.1.0",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"tailwind-merge": "^3.4.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@edge-runtime/vm": "^5.0.0",
		"@happy-dom/global-registrator": "^20.0.11",
		"@playwright/test": "^1.57.0",
		"@tailwindcss/postcss": "^4",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"bun-types": "^1.3.5",
		"concurrently": "^9.2.1",
		"convex-test": "^0.0.41",
		"pino-pretty": "^13.1.3",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5",
		"vitest": "^4.0.16"
	},
	"ignoreScripts": [
		"sharp",
		"unrs-resolver"
	],
	"trustedDependencies": [
		"sharp",
		"unrs-resolver"
	]
}
