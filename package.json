{
	"name": "vinci",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"dev:convex": "convex dev",
		"dev:all": "bun run dev & bun run dev:convex",
		"build": "next build",
		"start": "next start",
		"lint": "bunx biome check .",
		"lint:fix": "bunx biome check --write .",
		"format": "bunx biome format --write .",
		"typecheck": "tsc --noEmit",
		"test": "bun test",
