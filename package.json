		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"bun-types": "^1.3.5",
		"convex-test": "^0.0.41",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5",
		"vitest": "^4.0.16"
