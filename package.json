		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"convex": "^1.31.2",
		"lucide-react": "^0.562.0",
		"next": "16.1.1",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"tailwind-merge": "^3.4.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
